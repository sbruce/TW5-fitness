title: $:/plugins/sbruce/fitness/ui/stylesheets/WorkoutCalendarStyle
tags: $:/tags/Stylesheet
type: text/css

.calendar-table {
	border: 1px solid black;
  overflow: hidden;
  vertical-align: top;
  width: 100%;
}

.calendar-table table {
	width:100%;
}

.calendar-table td, th {
  width: 14%;
  vertical-align: top;
}

.calendar-table td {
  height: 5em;
}

.calendar-table th {
	height: 1em;
}

.date_today {
	color: green;
}
